                                        <li><a asp-area="" asp-controller="Catalogo" asp-action="Index" class="nav-item nav-link">Catalogo de las casas</a></li>
                                    <li><a asp-area="" asp-controller="Contactoes" asp-action="Index" class="nav-item nav-link">Mensajes</a></li>
                                        <li><a asp-area="" asp-controller="Sesion" asp-action="LogOut" class="nav-item nav-link">Cerrar Sesion</a></li>

                                        }

                                    }

                                </ul>
                           
                       </div>
