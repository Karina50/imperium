                                    @{
                                        @using Microsoft.AspNetCore.Http;

                                        if (string.IsNullOrEmpty(Context.Session.GetString("Sesion1")))
                                        {
                                            

                                            <li><a asp-area="" asp-controller="Home" asp-action="Index" class="nav-item nav-link">Inicio</a></li>
                                        <li><a asp-area="" asp-controller="Contactoes" asp-action="Create" class="nav-item nav-link">Contacto</a></li>
                                        <li><a asp-area="" asp-controller="Catalogo" asp-action="Index" class="nav-item nav-link">Catalogo de las casas</a></li>
                                        <li><a asp-area="" asp-controller="Sesion" asp-action="Index" class="nav-item nav-link">Inicio de sesion</a></li>

                                        }
                                        else
                                        {
                                           

                                        <li><a asp-area="" asp-controller="Home" asp-action="Index" class="nav-item nav-link">Inicio</a></li>
                                        <li><a asp-area="" asp-controller="Contactoes" asp-action="Create" class="nav-item nav-link">Contacto</a></li>
                                        <li><a asp-area="" asp-controller="RegistroUsers" asp-action="Create" class="nav-item nav-link">Registro Usuario</a></li>
