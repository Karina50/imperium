@model Imperium.Models.RegistroUser

@{
    ViewData["Title"] = "Index";
}

<head>
    <link rel="stylesheet" href="~/css/login.css" />
</head>


<div class="container">
    <div class="row">
        <div class="col-lg-3 col-md-2"></div>
        <div class="col-lg-6 col-md-8 login-box">
            <div class="col-lg-12 login-key">
                <i class="fa fa-key" aria-hidden="true"></i>
            </div>
            <div class="col-lg-12 login-title">
                ADMIN PANEL
            </div>

            <div class="col-lg-12 login-form">
                <div class="col-lg-12 login-form">
                    <form asp-action="Onpost" method="Post">
                        <div class="form-group">
                            <fieldset>
                                <label asp-for="Correo" class="control-label">Correo:</label>
                                <input asp-for="Correo" name="Cor" class="form-control" type="email" placeholder="Escriba su correo" required="" />
                                <span asp-validation-for="Correo" class="text-danger"></span>
                            </fieldset>
                        </div>
                        <div class="form-group">
                            <fieldset>
                                <label asp-for="Contra" class="control-label">Contraseña:</label>
                                <input asp-for="Contra" name="Con" class="form-control" type="password" placeholder="Escriba su contraseña" required="" />
                                <span asp-validation-for="Contra" class="text-danger"></span>
                            </fieldset>
                        </div>

                        <div class="col-lg-12 loginbttm">
                            <div class="col-lg-6 login-btm login-text">
                                <!-- Error Message -->
                            </div>
                            <div class="col-lg-6 login-btm login-button">
                                <button type="submit" id="form-submit" class="btn btn-outline-primary">Acceder</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="col-lg-3 col-md-2"></div>
        </div>
    </div>



